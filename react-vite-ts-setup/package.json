{
	"name": "react-vite-ts-setup",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"tests-env-setup": "yarn add -D ts-node @types/node @testing-library/react @testing-library/jest-dom @testing-library/user-event jest jest-environment-jsdom ts-jest @types/jest",
		"tests-babel-setup": "yarn add -D babel-jest @babel/preset-env @babel/preset-react @babel/preset-typescript",
		"tests-eslint-setup": "yarn add -D eslint-plugin-jest",
		"tests-jest-modules-setup": "yarn add -D jest-svg-transformer identity-obj-proxy",
		"tests-setup": "yarn tests-env-setup && yarn tests-jest-modules-setup && yarn tests-eslint-setup",
		"test": "jest"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^15.0.5",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.7",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "^8.57.0",
		"eslint-plugin-jest": "^28.3.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-svg-transformer": "^1.0.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	}
}
